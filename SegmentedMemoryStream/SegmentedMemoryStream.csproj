<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Sotiris Dragonas</Authors>
    <Company>-</Company>
    <Product>-</Product>
    <Description>An implementation of a memory stream with segmented storage so that the byte arrays do not end up in the LOH (Large Object Heap).</Description>
    <PackageId>Bazooka.SegmentedMemoryStream</PackageId>
    <PackageTags>MemoryStream,Stream,LOH,Segmented,</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <PropertyGroup>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
</Project>
